{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "jsx-no-multiline-js": false,
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": false
  }
}
